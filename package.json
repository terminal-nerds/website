{
	"$schema": "https://json.schemastore.org/package",
	"private": true,
	"name": "terminal-nerd",
	"homepage": "https://github.com/terminal-nerds/terminal-nerd",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/terminal-nerds/terminal-nerd.git"
	},
	"bugs": {
		"url": "https://github.com/terminal-nerds/terminal-nerd/issues"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^13.1.0",
		"commitlint": "^13.1.0",
		"markdownlint": "^0.23.1",
		"gacp": "^2.10.2",
		"prettier": "^2.3.2"
	},
	"scripts": {
		"commit": "gacp",
		"format": "prettier --write \".\"",
		"lint:commit": "commitlint --verbose --from \"develop\" --to \"HEAD\"",
		"lint:md": "markdownlint \"**/*.md\" --ignore \"node_modules/\""
	}
}
