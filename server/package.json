{
	"$schema": "https://json.schemastore.org/package",
	"private": true,
	"name": "@terminal-nerd/server",
	"homepage": "https://github.com/terminal-nerds/terminal-nerd/blob/main/server",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/terminal-nerds/terminal-nerd.git"
	},
	"bugs": {
		"url": "https://github.com/terminal-nerds/terminal-nerd/issues"
	},
	"type": "module",
	"devDependencies": {
		"@terminal-nerd/common": "workspace:*"
	},
	"scripts": {
		"build": "tsc --build",
		"clean:build": "rimraf ./build",
		"clean:node": "rimraf ./node_modules",
		"format": "prettier --write .",
		"lint:format": "prettier --check .",
		"lint:types": "tsc --noEmit",
		"start": "node ./build/example.js"
	}
}
